frontend www.realdavid.tech
	bind 10.0.0.3:80
	bind 10.0.0.3:443 ssl crt /etc/ssl/certs/realdavid.pem
	http-request redirect scheme https unless { ssl_fc }
	default_backend realdavid-tech-backend

backend realdavid-tech-backend
	balance roundrobin
    	server 128191-web-01 54.85.89.179:80 ssl
    	server 128191-web-02 54.237.27.97:80 ssl
	
